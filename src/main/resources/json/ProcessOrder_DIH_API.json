{
  "openapi": "3.0.0",
  "info": {
    "version": "1",
    "title": "Orders API",
    "description": "<p>This API provides resources for working with orders</p>",
    "x-targetEndpoint": "https://cpi-pntisdemo.it-cpi001-rt.cfapps.eu10.hana.ondemand.com/http/process_order"
  },
  "paths": {
    "/Orders": {
      "x-swagger-router-controller": "orders_ctrl",
      "post": {
        "parameters": [
          {
            "name": "CustomerName",
            "description": "The name of the customer",
            "in": "query",
            "required": true,
            "example": "Schmidt GmbH",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "CustomerID",
            "description": "The id of the customer",
            "in": "query",
            "required": true,
            "example": "10100010",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ProductID",
            "description": "The product id",
            "in": "query",
            "required": true,
            "example": "CM-FL-V00",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Quantity",
            "description": "The amount of products",
            "in": "query",
            "required": true,
            "example": "1",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Price",
            "description": "The price of the product",
            "in": "query",
            "required": true,
            "example": "8000",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "OrderReference",
            "description": "The order reference",
            "in": "query",
            "required": true,
            "example": "Gabelstapler V00",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "MaintenanceStartDate",
            "description": "The start date of the maintenance contract",
            "in": "query",
            "required": true,
            "example": "01.01.2019",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "MaintenanceEndDate",
            "description": "The end date of the maintenance contract",
            "in": "query",
            "required": true,
            "example": "01.01.2021",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "MaintenanceServiceLevel",
            "description": "The service level of the maintenance contract",
            "in": "query",
            "required": true,
            "example": "Premium",
            "schema": {
              "type": "string"
            }
          }
        ],
        "description": "Creates an order",
        "operationId": "write_order",
        "requestBody": {
          "required": true,
          "description": "New entity",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/orderResponse"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/orderResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "get": {
        "parameters": [
          {
            "name": "CustomerName",
            "description": "The name of the customer",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "CustomerID",
            "description": "The id of the customer",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ProductID",
            "description": "The distribution channel of order",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "description": "Searches for an order",
        "operationId": "read_order",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/orderResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "https://api-pntisdemo.prod.apimanagement.eu10.hana.ondemand.com:443/http/process_order"
